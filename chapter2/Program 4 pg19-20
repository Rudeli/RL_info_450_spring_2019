/* This program convers gallons to liters using 
floating point numbers. */

#include <iostream>
using namespace std;

int main()
{
	double gallons;
	double liters;

	cout << "Enter number of gallons: ";
	cin >> gallons; //This inputs from the user

	liters = gallons * 3.7854; //Convert to liters

	cout << "Liters: " << liters;
	
	return 0;
}
